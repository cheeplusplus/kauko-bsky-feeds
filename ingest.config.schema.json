{
    "type": "object",
    "properties": {
        "IngestConfig": {
            "type": "object",
            "properties": {
                "Verbose": {
                    "description": "Enable verbose logging",
                    "type": "boolean"
                },
                "ConsumeHistoricFeed": {
                    "description": "Consume historic feed, otherwise consume from tip of firehose",
                    "type": "boolean"
                },
                "SingleCollection": {
                    "description": "Single Jetstream collections to ingest. Use to override default.",
                    "type": "string"
                },
                "Filter": {
                    "description": "Filter certain collections by DIDs",
                    "type": "object",
                    "additionalProperties": {
                        "type": "object",
                        "properties": {
                            "ListUris": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            },
                            "Dids": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "MetricsUriPrefixes": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "format": "uri"
                    }
                }
            }
        }
    }
}